<%- include('../includes/header', { title: locals.container.title, sitename: locals.siteconfig.sitename}); %>

<div class="sector">
	<div class="container">
		<h1 class="headline"><%- locals.container.title %></h1>
	</div>
</div>

<div class="sector">
	<div class="container">
		<%- locals.container.content %>
	</div>
</div>

<% if(locals.latest_articles_show === true) { %>
	<div class="sector">
		<div class="container">
			<%- locals.latest_articles %>
		</div>
	</div>
<% } %>

<% if(locals.page.channel_articles && locals.page.channel_articles.length > 0) { %>
	<div class="sector">
		<div class="container">
			<p>
				<h5>
					Latest Articles:
				</h5>
				<ul>
					<% for(let i=0; i<locals.page.channel_articles.length; i++) { %>
						<li>
							this is an article
						</li>
					<% } %>
				</ul>
			</p>
		</div>
	</div>
<% } %>

<% if(locals.page.verbose > 3) { %>
	<div class="sector">
		<div class="container" style="background-color:papayawhip;">
			<h3>Debug</h3>
			<pre style="background-color:lightpink;">
			<%- JSON.stringify(locals, null, 4) %>	
			</pre>
		</div>
	</div>
<% } %>

<%- include('../includes/footer'); %>
